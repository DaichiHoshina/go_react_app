{
  "containerDefinitions": [
    {
      "name": "sample-webapp",
      "image": "326324184951.dkr.ecr.ap-northeast-1.amazonaws.com/frontend:latest",
      "cpu": 333,
      "memoryReservation": 600,
      "portMappings": [
        {
          "containerPort": 4567,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "command": ["bundle", "exec", "ruby", "app.rb", "-o", "0.0.0.0"],
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "DB_HOST"
        },
        {
          "name": "DB_USER",
          "valueFrom": "DB_USER"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "DB_PASSWORD"
        },
        {
          "name": "RACK_ENV",
          "valueFrom": "APP_ENV"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "sample-service",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "service"
        }
      }
    }
  ],
  "family": "webapp-service",
  "executionRoleArn": "arn:aws:iam::326324184951:role/ecs-task-role",
  "networkMode": "bridge",

  "volumes": [],

  "placementConstraints": []
}
